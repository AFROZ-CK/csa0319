#include <stdio.h>
int main() {
    int n, i, num, odd_sum = 0;
 printf("Enter the number of elements: ");
    scanf("%d", &n);
    printf("Enter %d numbers:\n", n);
    for (i = 1; i <= n; ++i) {
        scanf("%d", &num);
        if (num % 2 != 0) {  // Check for odd numbers
            odd_sum += num;
        }
    }
    printf("Sum of odd numbers = %d\n", odd_sum);
    return 0;
}
